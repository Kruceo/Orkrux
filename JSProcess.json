    "head": "\n    <route>forza</route>\n    <meta charset='utf-8'>\n    <meta http-equiv='X-UA-Compatible' content='IE=edge'>\n    <title>ISSO ai</title>\n    <meta name='viewport' content='width=device-width, initial-scale=1'>\n    <link    rel=  'stylesheet' type='text/css' media='screen' href='main.css'>\n    <style>\n        @import url('https://fonts.googleapis.com/css2?family=Fuzzy+Bubbles&display=swap');\n\n        @import url('https://fonts.googleapis.com/css2?family=Dongle&display=swap');\n    </style>\n"
  },
  "cssPaths": [],
  "jsPaths": [
    {
      "type": "",
      "code": "\n        window.onload = () => {\n            getSuspension();\n            document.querySelectorAll('input').forEach((inp, indice) => {\n                inp.onchange = function () {\n                    getSuspension();\n                }\n\n            }\n            )\n        }\n\n        function getSuspension() {\n            var peso = 1132;\n            peso = document.getElementById('peso-kg').value\n            var pesoLibras = peso * 2.2\n            var dianteira = 61;\n\n            dianteira = document.getElementById('dianteira').value\n\n            var traseira = 100 - dianteira;\n            var dianteiraResult = pesoLibras * (dianteira / 100)\n            var traseiraResult = pesoLibras * (traseira / 100)\n\n            var resultD = ((dianteiraResult) / 2) / 5.6\n            var resultT = ((traseiraResult) / 2) / 5.6\n\n            document.querySelector('#resultadoD').innerHTML = \"D: \" + resultD.toFixed(1) + ' kgf/mm';\n            document.querySelector('#resultadoT').innerHTML = 'T: ' + resultT.toFixed(1) + ' kgf/mm';\n            document.querySelector('#retornoD').innerHTML = \"D: \" + (((dianteiraResult / 2) / 100) + 1).toFixed(1) + ' points';\n            document.querySelector('#retornoT').innerHTML = 'T: ' + (((traseiraResult / 2) / 100) + 1).toFixed(1) + ' points'\n            document.querySelector('#compressaoD').innerHTML = 'D: ' + ((((dianteiraResult / 2) / 100) + 1) * 1.5).toFixed(1) + ' points'\n            document.querySelector('#compressaoT').innerHTML = 'T: ' + ((((traseiraResult / 2) / 100) + 1) * 1.5).toFixed(1) + ' points'\n        }\n    "
    }
  ],
  "route": "forza",
